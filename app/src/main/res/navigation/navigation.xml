<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.trip.tourvista.view.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.trip.tourvista.view.fragments.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/savedFragment"
        android:name="com.trip.tourvista.view.fragments.SavedFragment"
        android:label="fragment_saved"
        tools:layout="@layout/fragment_saved" />

    <fragment
        android:id="@+id/tourFragment"
        android:name="com.trip.tourvista.view.fragments.TourFragment"
        android:label="fragment_tour"
        tools:layout="@layout/fragment_tour" />

    <fragment
        android:id="@+id/allToursFragment"
        android:name="com.trip.tourvista.view.fragments.AllToursFragment"
        android:label="fragment_all_tour"
        tools:layout="@layout/fragment_all_tours" >
    </fragment>

</navigation>